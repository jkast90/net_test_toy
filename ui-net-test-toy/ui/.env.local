# Local development configuration for Netstream

# Network Configuration
DOMAIN=localhost
HOST=localhost

# Database Configuration
POSTGRES_DB=netstream_db
POSTGRES_USER=postgres
POSTGRES_PASSWORD=mypassword

# API Configuration
VITE_WORKSPACE_API_BASE_URL=http://localhost:5001
LOG_LEVEL=INFO

# Container Manager Configuration (optional - auto-detects http://localhost:5010)
# VITE_CONTAINER_MANAGER_URL=http://localhost:5010

# Protocol Configuration
ENABLE_HTTPS=false
DISABLE_AUTH=true

# Environment
NODE_ENV=development
